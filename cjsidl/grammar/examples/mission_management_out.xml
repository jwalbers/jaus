<?xml version='1.0' encoding='UTF-8'?>
<service_def name="MissionManagement" id="com:neya:mpms:MissionManagement" version="0.1" xmlns="urn:jaus:jsidl:1.0">
  <description>The Mission Management service provides the means to distribute,&#13;
	monitor, and control Mission Task Packages that allow assets to perform&#13;
	parts of a Mission Plan autonomously and collaboratively</description>
  <assumptions>All random deviations will tend in our favor.</assumptions>
  <declared_const_set name="Constants">
    <const_def name="MAX_JSIDL_LEN" const_type="unsigned integer" const_value="16384" field_units="one" interpretation="arbitrary"/>
    <const_def name="MAX_JC3IEDM_CODE" const_type="unsigned integer" const_value="6" field_units="one" interpretation="defined by JC3IEDM"/>
    <const_def name="MAX_JC3IEDM_VALUE" const_type="unsigned integer" const_value="64" field_units="one"/>
  </declared_const_set>
  <declared_type_set name="BasicTypes" id="urn:jaus:jss:core:MessageSet:BasicTypes" version="1.1">
    <fixed_field field_type="unsigned byte" name="AuthorityCode" field_units="one" optional="false">
      <value_set offset_to_lower_limit="false">
        <value_range lower_limit="0" lower_limit_type="inclusive" upper_limit="255" upper_limit_type="inclusive"/>
      </value_set>
    </fixed_field>
    <fixed_field field_type="unsigned short integer" name="EmergencyCode" field_units="one" optional="false">
      <value_set offset_to_lower_limit="false">
        <value_enum enum_const="STOP" enum_index="1"/>
      </value_set>
    </fixed_field>
    <bit_field name="TimeStamp" field_type_unsigned="unsigned integer" optional="false">
      <sub_field name="Milliseconds">
        <bit_range from_index="0" to_index="9"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="0" lower_limit_type="inclusive" upper_limit="999" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="Seconds">
        <bit_range from_index="10" to_index="15"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="0" lower_limit_type="inclusive" upper_limit="59" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="Minutes">
        <bit_range from_index="16" to_index="21"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="0" lower_limit_type="inclusive" upper_limit="59" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="Hour">
        <bit_range from_index="22" to_index="26"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="0" lower_limit_type="inclusive" upper_limit="23" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="Day">
        <bit_range from_index="27" to_index="31"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="1" lower_limit_type="inclusive" upper_limit="31" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
    </bit_field>
    <bit_field name="DateStamp" field_type_unsigned="unsigned short integer" optional="false">
      <sub_field name="Day">
        <bit_range from_index="0" to_index="4"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="1" lower_limit_type="inclusive" upper_limit="31" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="Month">
        <bit_range from_index="5" to_index="8"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="1" lower_limit_type="inclusive" upper_limit="12" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="Year">
        <bit_range from_index="9" to_index="15"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="0" lower_limit_type="inclusive" upper_limit="127" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
    </bit_field>
    <record name="TimeRec" optional="false" interpretation=" record TimeRec JausID">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_bit_field name="TimeStamp" declared_type_ref="TimeStamp" optional="true"/>
      <declared_bit_field name="DateStamp" declared_type_ref="DateStamp" optional="true"/>
    </record>
    <bit_field name="JausID" field_type_unsigned="unsigned integer" optional="false">
      <sub_field name="ComponentID">
        <bit_range from_index="0" to_index="7"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="1" lower_limit_type="inclusive" upper_limit="255" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="NodeID">
        <bit_range from_index="8" to_index="15"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="1" lower_limit_type="inclusive" upper_limit="255" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
      <sub_field name="SubsystemID">
        <bit_range from_index="16" to_index="31"/>
        <value_set offset_to_lower_limit="false">
          <value_range lower_limit="1" lower_limit_type="inclusive" upper_limit="65535" upper_limit_type="inclusive"/>
        </value_set>
      </sub_field>
    </bit_field>
    <fixed_field field_type="unsigned short integer" name="ElementUID" field_units="one" optional="false"/>
    <record name="ElementRec" optional="false" interpretation=" UID of the next (child) element in the list. The value is zero (0) if this is the last (tail) element. record ElementRec">
      <declared_fixed_field name="ElementUID" declared_type_ref="ElementUID" optional="false"/>
      <declared_fixed_field name="PreviousUID" declared_type_ref="ElementUID" optional="false"/>
      <declared_fixed_field name="NextUID" declared_type_ref="ElementUID" optional="false"/>
      <variable_format_field name="ElementData" optional="false" interpretation="variable_format_field ElementData">
        <format_field>
          <format_enum index="0" field_format="JAUS_MESSAGE"/>
          <format_enum index="1" field_format="User_defined"/>
        </format_field>
        <count_field field_type_unsigned="unsigned short integer"/>
      </variable_format_field>
    </record>
  </declared_type_set>
  <declared_type_set name="Types">
    <record name="ByteData" optional="false" interpretation=" record ByteData">
      <fixed_field field_type="unsigned byte" name="byte_field" optional="false" field_units="one"/>
    </record>
    <list name="ByteList" optional="false">
      <count_field field_type_unsigned="unsigned short integer" min_count="0" max_count="65535"/>
      <declared_record name="bytes" optional="false" declared_type_ref="ByteData"/>
    </list>
    <list name="ItemRef" optional="false">
      <count_field field_type_unsigned="unsigned short integer" min_count="0" max_count="65535"/>
      <declared_record name="item_index" optional="false" declared_type_ref="ByteData"/>
    </list>
    <record name="JausId" optional="false" interpretation="">
      <fixed_field field_type="unsigned short integer" name="subsystem_id" optional="false" field_units="one"/>
      <fixed_field field_type="unsigned byte" name="node_id" optional="false" field_units="one"/>
      <fixed_field field_type="unsigned byte" name="component_id" optional="false" field_units="one"/>
    </record>
    <list name="JausIdList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="jausId" optional="false" declared_type_ref="JausId"/>
    </list>
    <fixed_field field_type="unsigned integer" name="MissionUID" field_units="one" optional="false"/>
    <fixed_field field_type="unsigned integer" name="PackageUID" field_units="one" optional="false"/>
    <fixed_field field_type="unsigned integer" name="TaskUID" field_units="one" optional="false"/>
    <record name="MissionParametersSpec" optional="false" interpretation="">
      <declared_fixed_field name="missionId" declared_type_ref="MissionUID" optional="false"/>
      <variable_length_string name="jsidl" optional="false">
        <count_field min_count="0" max_count="MAX_JSIDL_LEN" field_type_unsigned="unsigned integer"/>
      </variable_length_string>
    </record>
    <sequence name="MissionParameterBinding" optional="false">
      <declared_list name="item" declared_type_ref="ItemRef" optional="false"/>
      <declared_list name="publishers" declared_type_ref="JausIdList" optional="false"/>
    </sequence>
    <list name="MissionParameterBindingList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_sequence name="binding" optional="false" declared_type_ref="MissionParameterBinding"/>
    </list>
    <list name="MissionParameterRefSet" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_list name="ref" optional="false" declared_type_ref="ItemRef"/>
    </list>
    <record name="MissionId" optional="false" interpretation="">
      <declared_fixed_field name="missionId" declared_type_ref="MissionUID" optional="false"/>
    </record>
    <sequence name="MissionParameterRefs" optional="false">
      <declared_record name="missionId" declared_type_ref="MissionId" optional="false"/>
      <declared_list name="refs" declared_type_ref="MissionParameterRefSet" optional="false"/>
    </sequence>
    <sequence name="MissionParameterAssignment" optional="false">
      <declared_list name="item" declared_type_ref="ItemRef" optional="false"/>
      <declared_list name="encodedValue" declared_type_ref="ByteList" optional="false"/>
    </sequence>
    <list name="MissionParameterValueSet" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_sequence name="assignment" optional="false" declared_type_ref="MissionParameterAssignment"/>
    </list>
    <record name="TimeRec" optional="false" interpretation=" time of value snapshot">
      <declared_bit_field name="timeStamp" declared_type_ref="TimeStamp" optional="false"/>
    </record>
    <sequence name="MissionParameterValues" optional="false">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_record name="missionId" declared_type_ref="MissionId" optional="false"/>
      <declared_list name="values" declared_type_ref="MissionParameterValueSet" optional="false"/>
      <declared_record name="timestamp" declared_type_ref="TimeRec" optional="true"/>
    </sequence>
    <record name="TaskId" optional="false" interpretation="">
      <declared_fixed_field name="taskId" declared_type_ref="TaskUID" optional="false"/>
    </record>
    <list name="TaskIdList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="taskId" optional="false" declared_type_ref="TaskId"/>
    </list>
    <sequence name="TslTask" optional="false">
      <declared_record name="taskId" declared_type_ref="TaskId" optional="false"/>
      <declared_list name="encodedTask" declared_type_ref="ByteList" optional="false"/>
    </sequence>
    <list name="TslTaskList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_sequence name="task" optional="false" declared_type_ref="TslTask"/>
    </list>
    <record name="PackageId" optional="false" interpretation="">
      <declared_fixed_field name="pid" declared_type_ref="PackageUID" optional="false"/>
    </record>
    <sequence name="StandardTaskPackage" optional="false">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_record name="missionId" declared_type_ref="MissionId" optional="false"/>
      <declared_record name="packageId" declared_type_ref="PackageId" optional="false"/>
      <declared_record name="missionParameters" declared_type_ref="MissionParametersSpec" optional="true"/>
      <declared_sequence name="initialParams" declared_type_ref="MissionParameterValues" optional="true"/>
      <declared_list name="publishers" declared_type_ref="MissionParameterBindingList" optional="true"/>
      <declared_list name="tasks" declared_type_ref="TslTaskList" optional="true"/>
      <declared_record name="main" declared_type_ref="TaskId" optional="true"/>
      <declared_list name="exceptionListeners" declared_type_ref="JausIdList" optional="true"/>
      <declared_list name="logListeners" declared_type_ref="JausIdList" optional="true"/>
      <declared_list name="requiredCapabilities" declared_type_ref="CapabilityList" optional="true"/>
    </sequence>
    <variant name="TaskPackage" optional="false">
      <vtag_field field_type_unsigned="unsigned byte"/>
      <declared_sequence name="stp" optional="false" declared_type_ref="StandardTaskPackage"/>
    </variant>
    <fixed_field field_type="unsigned byte" optional="false" name="RunStatusCode" field_units="one">
      <value_set offset_to_lower_limit="false">
        <value_enum enum_const="INVALID" enum_index="0"/>
        <value_enum enum_const="LOADED" enum_index="1"/>
        <value_enum enum_const="READY" enum_index="2"/>
        <value_enum enum_const="PAUSED" enum_index="3"/>
        <value_enum enum_const="RUNNING" enum_index="4"/>
        <value_enum enum_const="ABORTED" enum_index="5"/>
        <value_enum enum_const="ERROR" enum_index="6"/>
      </value_set>
    </fixed_field>
    <record name="MissionStatus" optional="false" interpretation=" Time of last status change.">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_fixed_field name="mission" declared_type_ref="MissionUID" optional="false"/>
      <declared_fixed_field name="status" declared_type_ref="RunStatusCode" optional="false"/>
      <variable_length_string name="extendedText" optional="true">
        <count_field min_count="0" max_count="MAX_STRING" field_type_unsigned="unsigned integer"/>
      </variable_length_string>
      <declared_bit_field name="time" declared_type_ref="TimeStamp" optional="true"/>
    </record>
    <record name="TaskStatus" optional="false" interpretation=" Time of last status change.">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_fixed_field name="tid" declared_type_ref="TaskUID" optional="false"/>
      <declared_fixed_field name="status" declared_type_ref="RunStatusCode" optional="false"/>
      <variable_length_string name="extendedText" optional="true">
        <count_field min_count="0" max_count="MAX_STRING" field_type_unsigned="unsigned integer"/>
      </variable_length_string>
      <declared_bit_field name="time" declared_type_ref="TimeStamp" optional="true"/>
    </record>
    <list name="TaskStatusList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="status" optional="false" declared_type_ref="TaskStatus"/>
    </list>
    <fixed_field field_type="unsigned byte" name="RequestId" field_units="one" optional="false"/>
    <record name="RequestIdRec" optional="false" interpretation="">
      <declared_fixed_field name="rid" declared_type_ref="RequestId" optional="false"/>
    </record>
    <list name="RequestIdList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="reqId" optional="false" declared_type_ref="RequestIdRec"/>
    </list>
    <record name="InfoSelection" optional="false" interpretation=" extended text and timestamps">
      <fixed_field field_type="unsigned byte" name="infoSelect" optional="false" field_units="one">
        <value_set offset_to_lower_limit="false">
          <value_enum enum_const="EXTENDED_TEXT" enum_index="1"/>
          <value_enum enum_const="TIMESTAMPS" enum_index="2"/>
          <value_enum enum_const="ALL_INFO" enum_index="3"/>
        </value_set>
      </fixed_field>
    </record>
    <sequence name="ExtendedErrorQuery" optional="false">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_list name="requestIds" declared_type_ref="RequestIdList" optional="false"/>
      <declared_record name="selection" declared_type_ref="InfoSelection" optional="true"/>
    </sequence>
    <record name="ErrorInfo" optional="false" interpretation=" Time of command/error.">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_fixed_field name="reqId" declared_type_ref="RequestId" optional="false"/>
      <fixed_field field_type="unsigned long integer" name="extendedErrorCode" optional="false" field_units="one"/>
      <variable_length_string name="extendedText" optional="true">
        <count_field min_count="0" max_count="MAX_STRING" field_type_unsigned="unsigned integer"/>
      </variable_length_string>
      <declared_bit_field name="time" declared_type_ref="TimeStamp" optional="true"/>
    </record>
    <list name="ErrorInfoList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="info" optional="false" declared_type_ref="ErrorInfo"/>
    </list>
    <fixed_field field_type="unsigned byte" optional="false" name="MissionManagementResponseCode" field_units="one">
      <value_set offset_to_lower_limit="false">
        <value_enum enum_const="SUCCESSFUL" enum_index="0"/>
        <value_enum enum_const="FAILED_GENERIC" enum_index="1"/>
        <value_enum enum_const="NOT_PERMITTED" enum_index="2"/>
        <value_enum enum_const="TRY_AGAIN" enum_index="3"/>
        <value_enum enum_const="NOT_CAPABLE" enum_index="4"/>
        <value_enum enum_const="INVALID_MISSION_ID" enum_index="5"/>
        <value_enum enum_const="INVALID_PACKAGE_ID" enum_index="6"/>
        <value_enum enum_const="INVALID_TASK_ID" enum_index="7"/>
        <value_enum enum_const="OTHER_INVALID_PARAMETER" enum_index="8"/>
      </value_set>
    </fixed_field>
    <sequence name="TaskExceptionQuery" optional="false">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_list name="taskIds" declared_type_ref="TaskIdList" optional="false"/>
      <declared_record name="numExceptions" declared_type_ref="ByteData" optional="false"/>
      <declared_record name="selection" declared_type_ref="InfoSelection" optional="true"/>
    </sequence>
    <record name="ExceptionInfo" optional="false" interpretation=" Time of command/error.">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_fixed_field name="tid" declared_type_ref="TaskUID" optional="false"/>
      <fixed_field field_type="unsigned long integer" name="exceptionCode" optional="false" field_units="one"/>
      <variable_length_string name="extendedText" optional="true">
        <count_field min_count="0" max_count="MAX_STRING" field_type_unsigned="unsigned integer"/>
      </variable_length_string>
      <declared_bit_field name="time" declared_type_ref="TimeStamp" optional="true"/>
    </record>
    <list name="TaskExceptionList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="info" optional="false" declared_type_ref="ExceptionInfo"/>
    </list>
    <fixed_field field_type="unsigned byte" optional="false" name="LogLevelVal" field_units="one">
      <value_set offset_to_lower_limit="false">
        <value_enum enum_const="LOG_EMERG" enum_index="0"/>
        <value_enum enum_const="LOG_ALERT" enum_index="1"/>
        <value_enum enum_const="LOG_CRIT" enum_index="2"/>
        <value_enum enum_const="LOG_ERR" enum_index="3"/>
        <value_enum enum_const="LOG_WARNING" enum_index="4"/>
        <value_enum enum_const="LOG_NOTICE" enum_index="5"/>
        <value_enum enum_const="LOG_INFO" enum_index="6"/>
        <value_enum enum_const="LOG_DEBUG" enum_index="7"/>
      </value_set>
    </fixed_field>
    <record name="LogLevel" optional="false" interpretation="">
      <declared_fixed_field name="level" declared_type_ref="LogLevelVal" optional="false"/>
    </record>
    <sequence name="TaskLogQuery" optional="false">
      <declared_list name="taskIds" declared_type_ref="TaskIdList" optional="false"/>
      <declared_record name="numEntries" declared_type_ref="ByteData" optional="false"/>
      <declared_record name="level" declared_type_ref="LogLevel" optional="false"/>
    </sequence>
    <record name="LogInfo" optional="false" interpretation=" Time of command/error.">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <declared_fixed_field name="tid" declared_type_ref="TaskUID" optional="false"/>
      <fixed_field field_type="unsigned byte" name="level" optional="false" field_units="one"/>
      <variable_length_string name="extendedText" optional="true">
        <count_field min_count="0" max_count="MAX_STRING" field_type_unsigned="unsigned integer"/>
      </variable_length_string>
      <declared_bit_field name="time" declared_type_ref="TimeStamp" optional="true"/>
    </record>
    <list name="TaskLogList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="info" optional="false" declared_type_ref="LogInfo"/>
    </list>
    <record name="Jc3iedmCapability" optional="false" interpretation=" TODO: later make variant">
      <presence_vector field_type_unsigned="unsigned byte"/>
      <fixed_length_string name="jc3Code" string_length="MAX_JC3IEDM_CODE" optional="false"/>
      <variable_length_string name="jc3Value" optional="true">
        <count_field min_count="0" max_count="MAX_JC3IEDM_VALUE" field_type_unsigned="unsigned integer"/>
      </variable_length_string>
    </record>
    <list name="CapabilityList" optional="false">
      <count_field field_type_unsigned="unsigned byte" min_count="0" max_count="255"/>
      <declared_record name="capability" optional="false" declared_type_ref="Jc3iedmCapability"/>
    </list>
  </declared_type_set>
  <message_set>
    <input_set>
      <message_def name="AddTaskPackage" message_id="0fa1" is_command="true">
        <description>Download a task package to the service</description>
        <header name="header">
          <record optional="false" name="HeaderRec">
            <fixed_field field_type="unsigned short integer" field_units="one" optional="false" name="MessageID" interpretation="Two byte field to hold message ID"/>
          </record>
        </header>
        <body name="body">
          <declared_variant name="package" optional="false" declared_type_ref="TaskPackage"/>
        </body>
        <footer name="footer"/>
      </message_def>
      <message_def name="UpdateTaskPackage" message_id="0fa2" is_command="true">
        <description>Selectively update contents of a loaded task package</description>
        <header name="header">
          <record optional="false" name="HeaderRec">
            <fixed_field field_type="unsigned short integer" field_units="one" optional="false" name="MessageID" interpretation="Two byte field to hold message ID"/>
          </record>
        </header>
        <body name="body">
          <declared_variant name="package" optional="false" declared_type_ref="TaskPackage"/>
        </body>
        <footer name="footer"/>
      </message_def>
      <message_def name="RemoveTaskPackage" message_id="0fa3" is_command="true">
        <description>Remove a loaded task package.</description>
        <header name="header">
          <record optional="false" name="HeaderRec">
            <fixed_field field_type="unsigned short integer" field_units="one" optional="false" name="MessageID" interpretation="Two byte field to hold message ID"/>
          </record>
        </header>
        <body name="body">
          <declared_record name="packageId" optional="false" declared_type_ref="TaskId"/>
        </body>
        <footer name="footer"/>
      </message_def>
      <message_def name="ActivateMission" message_id="0fa4" is_command="true">
        <description>Make designated Mission the active mission&#13;
                   and Run its main Task, if any.</description>
        <header name="header">
          <record optional="false" name="HeaderRec">
            <fixed_field field_type="unsigned short integer" field_units="one" optional="false" name="MessageID" interpretation="Two byte field to hold message ID"/>
          </record>
        </header>
        <body name="body">
          <declared_record name="mission" optional="false" declared_type_ref="MissionId"/>
        </body>
        <footer name="footer"/>
      </message_def>
      <message_def name="DectivateMission" message_id="0fa5" is_command="true">
        <description>Deactivate the designated Mission,&#13;
                   Abort and Reset all Tasks, unsubscribe&#13;
                   all parameter subscriptions, etc.</description>
        <header name="header">
          <record optional="false" name="HeaderRec">
            <fixed_field field_type="unsigned short integer" field_units="one" optional="false" name="MessageID" interpretation="Two byte field to hold message ID"/>
          </record>
        </header>
        <body name="body">
          <declared_record name="mission" optional="false" declared_type_ref="MissionId"/>
        </body>
        <footer name="footer"/>
      </message_def>
      <message_def name="AbortMission" message_id="0fa6" is_command="true">
        <description>Equivalent to sending AbortTask to all&#13;
                   Tasks in the mission.</description>
        <header name="header">
          <record optional="false" name="HeaderRec">
            <fixed_field field_type="unsigned short integer" field_units="one" optional="false" name="MessageID" interpretation="Two byte field to hold message ID"/>
          </record>
        </header>
        <body name="body">
          <declared_record name="mission" optional="false" declared_type_ref="MissionId"/>
        </body>
        <footer name="footer"/>
      </message_def>
      <message_def name="PauseMission" message_id="0fa7" is_command="true">
        <description>Equivalent to sending PauseTask to all&#13;
                   Tasks in the mission.</description>
        <header name="header">
